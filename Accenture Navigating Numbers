    "# Ensure 'Datetime' is in datetime format\n",
    "df['Datetime'] = pd.to_datetime(df['Datetime'])\n",
    "\n",
    "# Extract month and year from 'Datetime'\n",
    "df['Month'] = df['Datetime'].dt.to_period('M')\n",
    "\n",
    "# Count posts per month\n",
    "posts_per_month = df['Month'].value_counts()\n",
    "\n",
    "# Find the month with the most posts\n",
    "most_posts_month = posts_per_month.idxmax()\n",
    "most_posts_count = posts_per_month.max()\n",
    "\n",
    "print(f\"Month with the most posts: {most_posts_month}\")\n",
    "print(f\"Number of posts in that month: {most_posts_count}\")"
   ]
