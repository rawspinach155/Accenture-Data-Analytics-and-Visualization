     "start_time": "2024-09-10T01:45:01.056217",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Load the cleaned dataset\n",
    "df = pd.read_csv('Cleaned_Dataset.csv')\n",
    "\n",
    "# Aggregate scores for each category\n",
    "category_scores = df.groupby('Category')['Score'].sum().reset_index()\n",
    "\n",
    "# Sort categories by score\n",
    "category_scores = category_scores.sort_values(by='Score', ascending=False)\n",
    "\n",
    "# Identify the lowest 10 categories\n",
    "lowest_categories = category_scores.tail(10)\n",
    "\n",
    "# Combine lowest 10 categories into 'Others'\n",
    "others_score = lowest_categories['Score'].sum()\n",
    "top_categories = category_scores.head(-20)  # Exclude the lowest 20 categories\n",
    "\n",
    "# Create a DataFrame for 'Others'\n",
    "others_df = pd.DataFrame({'Category': ['Others'], 'Score': [others_score]})\n",
    "\n",
    "# Concatenate top categories with 'Others'\n",
    "final_df = pd.concat([top_categories, others_df], ignore_index=True)\n",
    "\n",
    "# Data for pie chart\n",
    "labels = final_df['Category']\n",
    "sizes = final_df['Score']\n",
    "colors = plt.cm.Paired(range(len(labels)))  # Use a colormap for variety\n",
    "explode = [0.1] * len(labels)  # Slightly explode all slices for better visibility\n",
    "\n",
    "# Create the pie chart\n",
    "plt.figure(figsize=(12, 8))\n",
    "plt.pie(sizes, explode=explode, labels=labels, colors=colors, autopct='%1.1f%%', shadow=False, startangle=140)\n",
    "plt.title('Proportion of Scores by Category (Lowest 10 Combined as \"Others\")')\n",
    "plt.show()"
   ]
  },
